{
	"name": "ssi",
	"displayName": "SSI",
	"description": "SSI language support",
	"icon": "images/package_icon.png",
	"license": "GPLv3",
	"version": "0.0.5",
	"repository": {
		"type": "git",
		"url": "https://github.com/cbspace/VSCodeExtensions/ssi"
	},
	"publisher": "CB",
	"engines": {
		"vscode": "^1.67.0"
	},
	"contributes": {
		"languages": [
			{
				"id": "ssi",
				"aliases": [
					"ssi"
				],
				"extensions": [
					".dat",
					".id"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ssi",
				"scopeName": "source.ssi",
				"path": "./syntaxes/ssi.tmLanguage.json"
			}
		]
	}
}
