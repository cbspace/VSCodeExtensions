{
	"name": "ssi",
	"displayName": "SSI",
	"description": "SSI, Westlock and Smartlock language support",
	"icon": "images/package_icon.png",
	"license": "GPLv3",
	"version": "0.0.7",
	"repository": {
		"type": "git",
		"url": "https://github.com/cbspace/VSCodeExtensions/ssi"
	},
	"publisher": "CB",
	"engines": {
		"vscode": "^1.67.0"
	},
	"contributes": {
		"languages": [
			{
				"id": "ssi",
				"aliases": [
					"ssi"
				],
				"extensions": [
					".dat",
					".id",
					".cip",
					".dia",
					".dio",
					".elt",
					".flg",
					".fop",
					".ipt",
					".map",
					".opt",
					".pfm",
					".prr",
					".pts",
					".qst",
					".rou",
					".sig",
					".tcs",
					".tfm",
					".tif",
					".but",
					".ind",
					".pbk",
					".pod",
					".ppd",
					".psd",
					".ptd"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ssi",
				"scopeName": "source.ssi",
				"path": "./syntaxes/ssi.tmLanguage.json"
			}
		]
	}
}
