{
	"name": "microlokii",
	"displayName": "Microlok II",
	"description": "Microlok II language support",
	"icon": "images/package_icon.png",
	"license": "GPLv3",
	"version": "0.0.35",
	"repository": {
		"type": "git",
		"url": "https://github.com/cbspace/VSCodeExtensions/microlokii"
	},
	"publisher": "CB",
	"engines": {
		"vscode": "^1.67.0"
	},
	"contributes": {
		"languages": [
			{
				"id": "microlokii",
				"aliases": [
					"microlokii"
				],
				"extensions": [
					".ml2",
					".mll",
					".gn2",
					".gnl",
					".mlk",
					".mls",
					".g2k",
					".gls"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "microlokii",
				"scopeName": "source.microlokii",
				"path": "./syntaxes/microlokii.tmLanguage.json"
			}
		]
	}
}
